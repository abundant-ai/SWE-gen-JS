-fn get_type_member_name(member: &AnyTsTypeMember) -> Option<AnyJsObjectMemberName> {
-    match member {
-        // Property signatures have names
-        AnyTsTypeMember::TsPropertySignatureTypeMember(prop) => prop.name().ok(),
