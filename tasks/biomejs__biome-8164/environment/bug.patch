-    }
-    fn diagnostic(ctx: &RuleContext<Self>, _state: &Self::State) -> Option<RuleDiagnostic> {
-        let interface = ctx.query();
-
-        Some(RuleDiagnostic::new(
-            category!("assist/source/useSortedInterfaceMembers"),
-            interface.range(),
