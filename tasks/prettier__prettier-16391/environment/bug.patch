-foo ?? bar ? a : b;
-a ? b : foo ?? bar;
-
-// Prettier stable
-foo ? bar ?? foo : baz;
-foo ?? bar ? a : b;
-a ? b : foo ?? bar;
-
-// Prettier main
-foo ? (bar ?? foo) : baz;
-(foo ?? bar) ? a : b;
-a ? b : (foo ?? bar);
