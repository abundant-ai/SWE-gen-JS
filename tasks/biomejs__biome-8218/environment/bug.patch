-    ///
-    /// ### Valid
-    ///
-    /// ```js
-    /// const foo = "Line 1\nLine 2";
-    /// ```
-    ///
-    /// ```js
-    /// const bar = `Line 1
-    /// Line 2`;
-    /// ```
-    pub NoMultiStr {
-        version: "next",
-        name: "noMultiStr",
-        language: "js",
-        recommended: false,
-        sources: &[RuleSource::Eslint("no-multi-str").same()],
-        severity: Severity::Error,
-    }
