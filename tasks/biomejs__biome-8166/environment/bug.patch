+///
+/// ```css
+/// supports(color: lch(7.1% 60.23 300.16)
+/// ```
+#[inline]
+fn parse_if_supports_test(p: &mut CssParser) -> ParsedSyntax {
+    if !is_at_if_supports_test(p) {
+        return Absent;
+    }
+
+    let m = p.start();
+    p.bump(T![supports]);
+    p.bump(T!['(']);
+
