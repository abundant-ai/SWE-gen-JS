+import static dev.nx.gradle.Groovy.nx
+
+nx(project) {
+  // Set project name
+  it.set 'name', 'my-custom-project-name'
+
+  // Add tags for organization
+  it.array 'tags', 'api', 'backend', 'tier:1'
+
+  // Add nested metadata
+  it.set 'metadata', {
+    it.set 'owner', 'team-payments'
+    it.set 'criticality', 'high'
+  }
+}
+```
+
+{% /tabitem %}
+{% /tabs %}
