+
+describe('add-jest-resolver migration', () => {
+  let tree: Tree;
+
+  beforeEach(() => {
+    tree = createTreeWithEmptyWorkspace();
+    // Create a minimal tsconfig.base.json at the workspace root for loadConfigFile
+    tree.write(
