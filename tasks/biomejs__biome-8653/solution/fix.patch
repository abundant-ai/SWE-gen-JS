-    /// // biome-ignore lint/nursery/noExcessiveLinesPerFile: generated file
-    /// const a = 1;
-    /// const b = 2;
-    /// const c = 3;
-    /// ```
-    ///
-    pub NoExcessiveLinesPerFile {
-        version: "next",
-        name: "noExcessiveLinesPerFile",
-        language: "js",
