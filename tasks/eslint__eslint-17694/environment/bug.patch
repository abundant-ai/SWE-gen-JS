+                "  /**\n" +
+                "   * @return {string} A string.\n" +
+                "   */\n" +
+                "  foo: function() {\n" +
+                "    return 'bar';\n" +
+                "  }\n" +
+                "});\n",
+            options: [{ requireReturn: false }]
+        },
+        {
+            code: "/**\n* Description\n* @return {void} */\nfunction foo(){}",
+            options: [{}]
+        },
+        {
